
# calculation of the energy levels. and radiative transition rates

SetAtom('Fe')
# 1s shell is closed
Closed('1s')
Config('2*8', group = 'n2')
Config('2*7 3*1', group = 'n3')

# Self-consistent iteration for optimized central potential
OptimizeRadial('n2', 'n3')
Structure('ne.d')

PrintTable('ne.d', 'ne.lev')
TransitionTable(['n2'], ['n3'], 'ne.tr', -1)

